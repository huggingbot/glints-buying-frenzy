{
  "name": "backend-buying-frenzy",
  "version": "1.0.0",
  "description": "Backend buying frenzy",
  "repository": "https://github.com/huggingbot/backend-buying-frenzy.git",
  "author": "huggingbot <83656073+huggingbot@users.noreply.github.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.13.0"
  },
  "scripts": {
    "start": "nodemon",
    "prebuild": "yarn lint",
    "build": "rimraf dist coverage *.tsbuildinfo && tsc -p tsconfig.build.json",
    "serve": "node  -r ./bootstrap.js dist/index.js",
    "lint": "tsc && eslint 'src/' --ignore-path .eslintignore --ext ts,js --max-warnings=0",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "setup": "sh bin/db-up.sh",
    "db:etl": "ts-node ./src/db_scripts/etl/index.ts",
    "db:gen-models": "ts-node ./src/db_scripts/model_definitions/gen_models.ts",
    "db:gen-migrations": "tsc -p tsconfig.migration.json",
    "db:drop": "npx sequelize db:drop",
    "db:create": "npx sequelize db:create",
    "db:migrate": "npx sequelize db:migrate --debug",
    "db:seed": "npx sequelize db:seed:all --debug",
    "db:reset": "yarn db:gen-models && yarn db:gen-migrations && yarn db:drop && yarn db:create && yarn db:migrate && yarn db:seed"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/jest": "~27.5",
    "@types/lodash": "^4.14.186",
    "@types/node": "~16",
    "@types/rimraf": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "~5.26",
    "@typescript-eslint/parser": "~5.26",
    "eslint": "~8.16",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "~26.2",
    "husky": "^8.0.1",
    "jest": "~28.1",
    "kill-port": "^2.0.1",
    "nodemon": "^2.0.20",
    "prettier": "~2.6",
    "rimraf": "~3.0",
    "ts-jest": "~28.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "tsutils": "~3.21",
    "typescript": "~4.7"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.1",
    "joi-to-swagger": "^6.1.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.37",
    "mysql2": "^2.3.3",
    "openapi-types": "^12.0.2",
    "sequelize": "^6.23.2",
    "sequelize-cli": "^6.5.1",
    "swagger-ui-express": "^4.5.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  }
}
